// FSP Header

typedef block FSPHeader {
    SPACE_ID: 4B;
    UNUSED: 4B;
    HIGHEST_PAGE_NO: 4B;                // size
    HIGHEST_PAGE_NO_INITIALIZED: 4B;    // free limit
    FLAGS: 4B;
    PAGES_USED_IN_FREE_FRAG: 4B;
    LIST_BASE_NODE_FOR_FREE: 16B;
    LIST_BASE_NODE_FOR_FREE_FRAG: 16B;
    LIST_BASE_NODE_FOR_FULL_FRAG: 16B;
    NEXT_UNUSED_SEGMENT_ID: 8B;
    LIST_BASE_NODE_FOR_FULL_INODES: 16B;
    LIST_BASE_NODE_FOR_FREE_INODES: 16B;
};